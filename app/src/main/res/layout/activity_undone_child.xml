<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.tale.qa.check_quiz.Activity_Undone_child">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ADD STUDENT QUIZ"
        android:id="@+id/add_StudentQuiz"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/listView3"
        android:layout_alignEnd="@+id/listView3" />

    <ListView
        android:layout_width="200dp"
        android:layout_height="match_parent"
        android:id="@+id/listView3"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/display_KTC" />

    <Spinner
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/display_KTC"
        android:layout_alignTop="@+id/add_StudentQuiz"
        android:layout_toLeftOf="@+id/add_StudentQuiz"
        android:layout_toStartOf="@+id/add_StudentQuiz" />


</RelativeLayout>
